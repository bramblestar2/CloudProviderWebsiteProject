(cloudflare_dns) {
	tls {
		dns cloudflare {env.CLOUDFLARE_API_TOKEN}
	}
}
www.saluki-smash.com {
	import cloudflare_dns
	redir https://saluki-smash.com{uri} 308
}
saluki-smash.com {
	import cloudflare_dns
	root * "C:\Tools\Landing Page"
	file_server
	encode gzip
	}

